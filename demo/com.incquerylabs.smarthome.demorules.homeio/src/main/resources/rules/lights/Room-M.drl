package homeioexample


rule "M Lights Switch"
    when
        $item : Item( name == "M_Lights_Analog", $state : state)

        $itemState : ItemStateChangedEvent( name == "M_Light_Switch", newState == OPEN)
    then
        if ($state.equals(HUNDRED)) {
            openhab.postCommand($item, OFF);
        } else {
            openhab.postCommand($item, ON);
        }
end